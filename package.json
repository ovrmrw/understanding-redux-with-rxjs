  "main": "index.js",
  "scripts": {
    "postinstall": "node lib/overwrite-modules.js",
    "ts": "ts-babel-node redux-rxjs.ts",
    "ts:alt": "node --require ts-babel-node/register redux-rxjs.ts",
    "tsc": "tsc --outDir .dest-tsc",
    "tsc:w": "tsc --outDir .dest-tsc --watch",
